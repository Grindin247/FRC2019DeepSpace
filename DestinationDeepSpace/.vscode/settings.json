{
  "java.configuration.updateBuildConfiguration": "automatic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "wpilib.online": true,

  "java.test.config": [
    {
        "name": "Unit Test",
        "workingDirectory": "${workspaceFolder}",
        "args": [ ],
        "vmargs": [ "-Xmx512M" ],
        "env": { },
    },
  ],
  "java.test.defaultConfig": "Unit Test"
}
